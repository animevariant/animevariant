{
  "name": "valiantlynx-turborepo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo run dev --concurrency=11",
    "python": "turbo run python",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "turbo format",
    "preview": "turbo preview",
    "package": "turbo package",
    "release": "turbo release",
    "docker": "turbo docker",
    "pull": "turbo pull",
    "push": "turbo push"
  },
  "devDependencies": {
    "@valiantlynx/tsconfig": "^0.0.25",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "turbo": "latest"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/scripts/python/*",
    "packages/scripts/python/school/math/*"
  ],
  "packageManager": "yarn@1.22.19",
  "usePackagemanager": "yarn@1.22.19 or pnpm@8.6.10 or npm@8.19.4",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  }
}
